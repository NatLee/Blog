{% if theme.like_coin.enable %}
<!-- like coin -->
<div>
    <iframe width="100%" height="230px" scrolling="no" frameborder="0" id="player" src="https://button.like.co/in/embed/{{ theme.like_coin.id }}/button/?referrer=<%- post.permalink %>" allowfullscreen="true"></iframe>
<div>
{% endif %}

{% if theme.google_adsense.postfooter_enable %}
<div class="post-end-adsense">
    <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <!-- typeart_post_body_end -->
    <ins class="adsbygoogle"
         style="display:block"
         data-ad-client="{{ theme.google_adsense.google_ad_client_id }}"
         data-ad-slot="{{ theme.google_adsense.postfooter_data_ad_slot }}"
         data-ad-format="auto"
         data-full-width-responsive="true"></ins>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({});
    </script>
</div>
{% endif %}